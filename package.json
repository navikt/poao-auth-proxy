  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^16.6.2",
    "@types/openid-client": "^3.7.0",
    "@types/url-join": "^4.0.1",
    "husky": "^7.0.1",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
