{
  "name": "poao-auth-proxy",
  "version": "1.0.0",
  "description": "Auth proxy ",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "prettier": "prettier --write 'src/**/*.ts'",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/poao-auth-proxy.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/navikt/poao-auth-proxy/issues"
  },
  "homepage": "https://github.com/navikt/poao-auth-proxy#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
